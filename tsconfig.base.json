{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2022", "DOM", "DOM.Iterable"],
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "jsx": "react-jsx",
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "noImplicitOverride": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "isolatedModules": true,
      "types": [],
      "baseUrl": ".",
      "paths": {
        "@core/*": ["packages/core/src/*"]
      }
    }
  }
  